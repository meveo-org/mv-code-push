      "script" : "package org.meveo.codepush;\r\n\r\nimport java.util.List;\r\nimport java.util.Map;\r\n\r\nimport org.slf4j.Logger;\r\nimport org.slf4j.LoggerFactory;\r\nimport org.meveo.service.script.Script;\r\nimport org.meveo.admin.exception.BusinessException;\r\nimport org.meveo.model.customEntities.MobileAppPackage;\r\nimport org.meveo.model.customEntities.MobileAppDeployment;\r\nimport org.meveo.model.customEntities.MobileAppDeploymentVersion;\r\n\r\nimport org.meveo.model.storage.Repository;\r\nimport org.meveo.service.storage.RepositoryService;\r\nimport org.meveo.api.persistence.CrossStorageApi;\r\nimport org.meveo.api.persistence.CrossStorageRequest;\r\nimport org.meveo.api.exception.EntityDoesNotExistsException;\r\n\r\n\r\nimport org.meveo.commons.utils.ParamBean;\r\n\r\npublic class CheckUpdate extends Script {\r\n    private static final Logger log = LoggerFactory.getLogger(CheckUpdate.class);\r\n      \r\n  \tprivate CrossStorageApi crossStorageApi = getCDIBean(CrossStorageApi.class);\r\n    private RepositoryService repositoryService = getCDIBean(RepositoryService.class);\r\n    private Repository defaultRepo = repositoryService.findDefaultRepository();\r\n  \r\n   \r\n    private String baseUrl = ParamBean.getInstance().getProperty(\"meveo.admin.baseUrl\",\"./\");\r\n    private String webcontext = ParamBean.getInstance().getProperty(\"meveo.admin.webContext\",\"meveo\");\r\n  \r\n    private String deploymentKey;\r\n    private String appVersion;\r\n    private String label;\r\n    private String packageHash;\r\n    private String clientUniqueId;\r\n    private String result;\r\n  \r\n    public void setDeploymentKey(String deploymentKey){\r\n      this.deploymentKey = deploymentKey;\r\n    }\r\n  \r\n    public void setAppVersion(String appVersion){\r\n      this.appVersion = appVersion;\r\n    }\r\n\t\r\n    public void setLabel(String label){\r\n      this.label = label;\r\n    }\r\n  \r\n    public void setPackageHash(String packageHash){\r\n      this.packageHash = packageHash;\r\n    }\r\n  \r\n    public void setClientUniqueId(String clientUniqueId){\r\n      this.clientUniqueId = clientUniqueId;\r\n    }\r\n  \r\n    public String getResult(){\r\n      return result;\r\n    }\r\n  \r\n\t@Override\r\n\tpublic void execute(Map<String, Object> parameters) throws BusinessException {\r\n      String packageId=\"0\";\r\n      String downloadURL=\"\";\r\n      boolean isAvailable=false;\r\n      boolean isDisabled=true;\r\n      boolean isMandatory=false;\r\n      long size=0;\r\n      if(packageHash!=null){\r\n        try {\r\n          MobileAppPackage appPackage = crossStorageApi.find(defaultRepo, MobileAppPackage.class).by(\"packageHash\", packageHash).getResult();\r\n          packageId=appPackage.getUuid();\r\n          downloadURL=baseUrl+webcontext+\"/api/rest/fileSystem/binaries/default/MobileAppPackage/\"+packageId+\"/packageContent\";\r\n          isAvailable=true;\r\n          isDisabled=appPackage.getDisabled();\r\n          isMandatory=appPackage.getMandatory();\r\n          packageHash=appPackage.getPackageHash();\r\n          size=appPackage.getSize();\r\n        } catch (Exception e){\r\n        }\r\n      } else {\r\n         int appVersionInt = Integer.parseInt(appVersion.replaceAll(\"\\\\D+\",\"\"));\r\n        try {\r\n          MobileAppDeployment deployment = crossStorageApi.find(defaultRepo, MobileAppDeployment.class).by(\"deploymentKey\", deploymentKey).getResult();\r\n          if(deployment!=null){\r\n            CrossStorageRequest<MobileAppDeploymentVersion> csreq = crossStorageApi.find(defaultRepo, MobileAppDeploymentVersion.class).by(\"deployment\",\r\n        \t\tdeployment.getUuid());\r\n    \t\tList<MobileAppDeploymentVersion> deploymentVersions = csreq.getResults();\r\n            MobileAppDeploymentVersion newVersion = null;\r\n            for(MobileAppDeploymentVersion deploymentVersion:deploymentVersions){\r\n               if(deploymentVersion.getMinVersion()==null ||  appVersionInt>=Integer.parseInt(deploymentVersion.getMinVersion().replaceAll(\"\\\\D+\",\"\"))){\r\n               \tif(deploymentVersion.getMaxVersion()==null ||  appVersionInt<=Integer.parseInt(deploymentVersion.getMaxVersion().replaceAll(\"\\\\D+\",\"\"))){\r\n                  newVersion=deploymentVersion;\r\n               \t}\r\n               }\r\n            }\r\n            if(newVersion!=null){\r\n              try {\r\n                appVersion = newVersion.getApplicationVersion();\r\n                log.info(\"newVersion:{}\",newVersion);\r\n                MobileAppPackage appPackage = crossStorageApi.find(defaultRepo, MobileAppPackage.class).by(\"uuid\", newVersion.getCurrentPackageUuid()).getResult();\r\n                packageId=appPackage.getUuid();\r\n                log.info(\"packageId:{}\",packageId);\r\n                isAvailable=true;\r\n          \t\tisDisabled=appPackage.getDisabled();\r\n          \t\tisMandatory=appPackage.getMandatory();\r\n                packageHash=appPackage.getPackageHash();\r\n                size=appPackage.getSize();\r\n                downloadURL=baseUrl+webcontext+\"/api/rest/fileSystem/binaries/default/MobileAppPackage/\"+packageId+\"/packageContent\";\r\n              } catch (Exception e){\r\n              }\r\n            }\r\n          }\r\n        } catch (Exception e){\r\n        }\r\n      }\r\n\t\tresult=\"{\\n\"\r\n          +\"\\\"packageId\\\": \\\"\"+packageId+\"\\\",\\n\"\r\n    \t  +\"\\\"downloadURL\\\": \\\"\"+downloadURL+\"\\\",\\n\"\r\n    \t  +\"\\\"downloadUrl\\\": \\\"\"+downloadURL+\"\\\",\\n\"\r\n    \t  +\"\\\"description\\\": \\\"\\\",\\n\"\r\n    \t  +\"\\\"isAvailable\\\": \"+isAvailable+\",\\n\"\r\n    \t  +\"\\\"isDisabled\\\": \"+isDisabled+\",\\n\"\r\n    \t  +\"\\\"isMandatory\\\": \"+isMandatory+\",\\n\"\r\n    \t  +\"\\\"appVersion\\\": \"+appVersion+\",\\n\"\r\n    \t  +\"\\\"targetBinaryRange\\\": \\\"\\\",\\n\"\r\n    \t  +\"\\\"packageHash\\\": \\\"\"+packageHash+\"\\\",\\n\"\r\n    \t  +\"\\\"label\\\": \\\"\\\",\\n\"\r\n    \t  +\"\\\"packageSize\\\": \"+size+\",\\n\"\r\n    \t  +\"\\\"updateAppVersion\\\": false,\\n\"\r\n    \t  +\"\\\"shouldRunBinaryVersion\\\": false,\\n\"\r\n    \t  +\"\\\"rollout\\\": 100\\n\"\r\n          +\"}\";\r\n      \r\n\t}\r\n\t\r\n}",
